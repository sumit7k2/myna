{
  "name": "expo-ts-starter",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "dev": "expo start",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "codegen": "graphql-codegen --config codegen.ts",
    "build:ios": "eas build -p ios --profile preview",
    "build:android": "eas build -p android --profile preview"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.11.0",
    "@react-navigation/bottom-tabs": "^6.6.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@shopify/flash-list": "^1.6.4",
    "@tanstack/react-query": "^5.56.2",
    "expo": "^51.0.0",
    "expo-file-system": "~17.0.1",
    "expo-image": "~1.13.0",
    "expo-image-picker": "~15.0.5",
    "expo-notifications": "~0.28.17",
    "expo-secure-store": "~13.0.2",
    "graphql": "^16.9.0",
    "msw": "^2.3.1",
    "react": "18.2.0",
    "react-native": "0.76.0",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-mmkv": "^3.1.0",
    "react-native-reanimated": "^3.10.1",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "^3.32.0",
    "react-native-svg": "^15.2.0",
    "sentry-expo": "^7.0.3",
    "tamagui": "^1.114.21",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.1.0",
    "@graphql-codegen/typescript-operations": "^4.3.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@react-native/eslint-config": "^0.76.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.3",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-native": "^0.76.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-tamagui": "^1.114.21",
    "babel-preset-expo": "^11.0.6",
    "babel-plugin-module-resolver": "^5.0.2",
    "detox": "^20.13.3",
    "dotenv": "^16.4.5",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-expo": "^51.0.1",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.ts"],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
